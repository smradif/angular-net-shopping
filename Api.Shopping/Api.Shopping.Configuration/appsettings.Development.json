{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AppSettings": {
    "SiteName": "First Shop",
    "AllowedHosts": [ "http://localhost:4200", "http://localhost:3000" ],
    "DefaultCurrency": "$",
    "ApiUrls": {
      "Auth": {
        "Authenticate": "https://localhost:49155/authenticate",
        "RefreshToken": "https://localhost:49155/refresh-token",
        "Verify": "https://localhost:49155/verify",
        "Signout": "https://localhost:49155/signout"
      },
      "Catalogue": {
        "GetMenu": "https://localhost:44380/menu",
        "GetProducts": "https://localhost:44380/Products/{productKey}",
        "GetProduct": "https://localhost:44380/Products/{productKey}/{productId}",
        "GetImage": "https://localhost:44380/Images/{productKey}/{imageName}",
        "GetBasketItems": "https://localhost:44380/Basket"
      },
      "Payment": {
        "GetMethods": "https://localhost:44350/Payments/methods",
        "GetCost": "https://localhost:44350/Payments/cost/{method}/{amount}"
      }
    },
    "IsDevelopment": true,
    "SwaggerVersion": "v1",
    "SwaggerName": "Api Shopping Authentication",
    "SocialItems": [
      {
        "name": "twitter",
        "url": "https://twitter.com",
        "text":  "Tweet"
      },
      {
        "name": "facebook",
        "url": "https://facebook.com",
        "text": "Share"
      },
      {
        "name": "instagram",
        "url": "https://instagram.com",
        "text": "Pin"
      }
    ]
  }
}
